      { "name": "sy", "type": "int", "value": "y % h" },
  
      { "name": "tE", "type": "int", "value": "min((o-idxE)%4, (idxE-o)%4)" },
      { "name": "tN", "type": "int", "value": "min((o-idxN)%4, (idxN-o)%4)" },
      { "name": "tW", "type": "int", "value": "min((o-idxW)%4, (idxW-o)%4)" },
      { "name": "tS", "type": "int", "value": "min((o-idxS)%4, (idxS-o)%4)" }
    ]
  }
  